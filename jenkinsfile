properties([pipelineTriggers([githubPush()])])

node('linux'){
    stage('test'){
        git 'https://github.com/akrizma12/java-project.git'
        sh "env"
        sh "ant -f test.xml -v"
        junit 'reports/result.xml'
    }
    stage('build'){
        sh "ant -f build.xml -v"
    }
    stage('deploy'){
        sh "aws s3 cp ${WORKSPACE}/distrectangle-${BUILD_NUMBER}.jar s3://karishma-assignment-4/" 
    }
    stage('report'){
    
    }

}
